<Page x:Class="HCI_Project.Pages.AdminPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:HCI_Project.Pages"
      mc:Ignorable="d" 
      Title="AdminPage">

    <Canvas Width="600" Height="600">


        <Path
        Name="UIPath"
        Fill="White"
        Stroke="DarkGray"
        StrokeThickness="2" HorizontalAlignment="Center" Canvas.Top="-1.415" VerticalAlignment="Top">
            <Path.Data>
                <PathGeometry>
                    <PathFigure StartPoint="200,0">
                        <LineSegment Point="0,150" />
                        <LineSegment Point="0,450" />
                        <LineSegment Point="200,600" />
                        <LineSegment Point="400,600" />
                        <LineSegment Point="600,450" />
                        <LineSegment Point="600,150" />
                        <LineSegment Point="400,0" />

                    </PathFigure>
                </PathGeometry>
            </Path.Data>
        </Path>
    </Canvas>
</Page>
